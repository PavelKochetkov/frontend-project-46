{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "import/extensions": "off",
        "no-console":"off",
        "no-underscore-dangle":"off",
        "no-else-return":"off"
    }
}
